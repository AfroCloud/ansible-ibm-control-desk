# Web Servers
web_server ansible_host=<change-ip> ansible_connection=ssh ansible_user=<change-user> ansible_ssh_pass=<change-password>

# DB Server
db_server ansible_host=<change-ip> ansible_connection=ssh ansible_user=<change-user> ansible_ssh_pass=<change-password>

# Application Server
app_server1 ansible_host=<change-ip> ansible_connection=ssh ansible_user=<change-user> ansible_ssh_pass=<change-password>

[all:vars]
repository_path = <path-where-your-installers-are>
tmp_dir = <path-shared-between-servers>
red_hat_username = red-hat-username
red_hat_password = red-hat-password
ihs_hostname = ihs-hostname
db2_hostname = db2-hostname
was_hostname = was-hostname
ihs_ip = ihs-ip
db2_ip = db2-ip
was_ip = was-ip
