<?xml version='1.0' encoding='UTF-8'?>
<agent-input>
  <variables>
    <variable name='sharedLocation' value='{{ __imshared_location }}'/>
  </variables>
  <server>
    <repository location='{{ __tmp_dir }}/Install/MWInstallerRepository'/>
    <repository location='{{ __tmp_dir }}/Install/mwi_madt_WASv9.0.0.7_images'/>
    <repository location='{{ __tmp_dir }}/Install/ControlDeskRepo'/>
  </server>
  <profile id='IBM   Tivoli&apos;s process automation suite' installLocation='{{ __smp_install_location }}'>
    <data key='cic.selector.arch' value='x86_64'/>
    <data key='user.productName,com.ibm.tivoli.pae.j2ee' value='IBM  Tivoli&apos;s process automation suite configuration tool for WebSphere Application Server'/>
    <data key='user.j2ee.installLocation,com.ibm.tivoli.pae.j2ee' value='{{ __smp_install_location }}'/>
    <data key='user.was.installLocation,com.ibm.tivoli.pae.j2ee' value='{{ __was_install_location }}'/>
    <data key='user.productVersion,com.ibm.tivoli.pae.j2ee' value='9.0.7.20180302_0440'/>
  </profile>
  <install>
    <!-- IBM  Tivoli's process automation suite configuration tool for WebSphere Application Server 9.0.0.7 -->
    <offering profile='IBM   Tivoli&apos;s process automation suite' id='com.ibm.tivoli.pae.j2ee' version='7.6.1.20180719-0029' features='main.feature,TPAEWiin32bitSupport.feature'/>
  </install>
  <profile id='IBM WebSphere Application Server V9.0' installLocation='{{ __was_install_location }}'>
    <data key='cic.selector.arch' value='x86_64'/>
  </profile>
  <install>
    <!-- IBM WebSphere Application Server Network Deployment  9.0.0.7 -->
    <offering profile='IBM WebSphere Application Server V9.0' id='com.ibm.websphere.ND.v90' version='9.0.7.20180302_0440' features='core.feature,ejbdeploy,thinclient,embeddablecontainer'/>
    <!-- IBM SDK, Java Technology Edition, Version 8 8.0.5.16 -->
    <offering profile='IBM WebSphere Application Server V9.0' id='com.ibm.java.jdk.v8' version='8.0.5016.20180606_1258' features='com.ibm.sdk.8'/>
  </install>
  <preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='${sharedLocation}'/>
</agent-input>
